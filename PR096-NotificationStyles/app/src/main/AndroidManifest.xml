<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.iessaladillo.pedrojoya.pr096" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="es.iessaladillo.pedrojoya.pr096.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="es.iessaladillo.pedrojoya.pr096.ResultadoActivity"
            android:label="@string/title_activity_resultado"
            android:parentActivityName="es.iessaladillo.pedrojoya.pr096.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="es.iessaladillo.pedrojoya.pr096.MainActivity" />
            <intent-filter>
                <action android:name="es.iessaladillo.pedrojoya.pr096.action.VIEW" />
                <action android:name="es.iessaladillo.pedrojoya.pr096.action.SEND" />
                <action android:name="es.iessaladillo.pedrojoya.pr096.action.DELETE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
