<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.iessaladillo.pedrojoya.pr027">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="es.iessaladillo.pedrojoya.pr027.actividades.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="es.iessaladillo.pedrojoya.pr027.actividades.AlumnoActivity"
            android:label="@string/title_activity_alumno"
            android:parentActivityName=".actividades.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".actividades.MainActivity" />
        </activity>
    </application>

</manifest>